{
  "name": "@web-remote-control/core",
  "version": "0.2.1",
  "description": "Core library of @web-remote-control",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "./node_modules/.bin/tsc -p ./tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepublish": "npm test && npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "example:browser": "webpack-dev-server --mode development",
    "example:node": "nodemon",
    "deploy": "gh-pages -d examples/dist",
    "publish-demo": "yarn build && yarn run deploy",
    "v:patch": "npm version patch --force",
    "v:minor": "npm version minor --force",
    "v:major": "npm version major --force"
  },
  "repository": "https://github.com/Alireza29675/web-remote-control/core",
  "author": "Alireza Sheikholmolouki <alireza.sheikholmolouki@gmail.com> (https://github.com/Alireza29675)",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@types/chalk": "^2.2.0",
    "@types/clear": "^0.1.0",
    "@types/jest": "^24.0.15",
    "@types/moment": "^2.13.0",
    "babel-loader": "7.1.4",
    "chalk": "^2.4.2",
    "clear": "^0.1.0",
    "css-loader": "^2.1.1",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "moment": "^2.24.0",
    "prettier": "^1.18.2",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3",
    "webpack-dev-server": "^3.7.1"
  },
  "dependencies": {
    "@types/md5": "^2.1.33",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/store": "^2.0.2",
    "md5": "^2.2.1",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "store": "^2.0.12"
  }
}
